;===================github=============
define condition Github_create_file
url.domain="github.com" url.path.substring="/new/main"
url.domain="github.com" url.path.substring="/new/master"
end
 
define condition Github_delete_file
url.domain="github.com" url.path.substring="/delete/main"
url.domain="github.com" url.path.substring="/delete/master"
end
 
 <proxy>
condition=Github_create_file DENY
condition=Github_delete_file DENY
;=============================================
